	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable.declaration": {
			"bold": false
		},
		"variable.decorator": "#FF5370",
		"method.decorator": "#FF5370",
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#839496"
			}
		},
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded",
				"string meta.image.inline.markdown"
			],
			"settings": {
				"foreground": "#839496"
			}
		},
		// Comment
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#646669"
			}
		},
		// String
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#FAAE0A"
			}
		},
		// Regexp
		{
			"name": "Regexp",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#E3090B"
			}
		},
		// Number
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#CF5D00"
			}
		},
		// Variable
		{
			"name": "Variable",
			"scope": [
				"variable.language",
				"variable.other"
			],
			"settings": {
				"foreground": "#D5D7D7"
			}
		},
		// Keyword
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#E2CD14"
			}
		},
		// Operator
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#E3090B"
			}
		},
		// Storage
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#93A1A1"
			}
		},
		// Class name
		{
			"name": "Class name",
			"scope": [
				"entity.name.class",
				"entity.name.type",
				"entity.name.namespace",
				"entity.name.scope-resolution"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FAAE0A"
			}
		},
		// function name
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#E2CD14"
			}
		},
		// Punctuation - variable start
		{
			"name": "Variable start",
			"scope": "punctuation.definition.variable",
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		// Embedded code markers
		{
			"name": "Embedded code markers",
			"scope": [
				"punctuation.section.embedded.begin",
				"punctuation.section.embedded.end"
			],
			"settings": {
				"foreground": "#E3090B"
			}
		},
		// Built in constant
		{
			"name": "Built-in constant",
			"scope": [
				"constant.language",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#B58900"
			}
		},
		// Support construct
		{
			"name": "Support.construct",
			"scope": [
				"support.function.construct",
				"keyword.other.new"
			],
			"settings": {
				"foreground": "#CB4B16"
			}
		},
		// User-defined constant
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#CB4B16"
			}
		},
		// Inherited class
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#6C71C4"
			}
		},
		// Function argument
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {}
		},
		// Tag name
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
